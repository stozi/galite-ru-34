<!-- Paste this into the <layoutList> section of
     your evdev.xml to make the layout appear in GNOME
     input sources under 'English (US)'.
     Usually to find at: /usr/share/X11/xkb/rules/evdev.xml
-->

<layout>
    <configItem>
        <name>gallium-v2</name>
        <shortDescription>gallium-v2</shortDescription>
        <description>Gallium-v2</description>
        <languageList>
            <iso639Id>eng</iso639Id>
        </languageList>
    </configItem>
</layout>
